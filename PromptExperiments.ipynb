    "from google import genai\n",
    "import os\n",
    "import time\n",
    "import pandas as pd"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 22,
   "id": "ec53cd01-5c2d-451b-8d77-cc9a24a4d098",
   "metadata": {},
   "outputs": [],
   "source": [
    "client = genai.Client(api_key=\"API KEY")\n",
    "MODEL_NAME = \"gemini-1.5-flash\""
   ]
  },
  {
   "cell_type": "code",

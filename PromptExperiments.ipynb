   "id": "e004fe43-f898-483e-a709-6773db3e988b",
   "metadata": {},
   "outputs": [],
   "source": [
    "df = pd.read_csv('C:/Users/DELISHA.000/Desktop/prompt-engineering-for-data-analytics/sales_summary.csv')\n",
    "fact_summary = df.to_string(index=False)"
   ]
  },
  {
   "cell_type": "code",
